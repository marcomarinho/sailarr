# ============================================
# GENERAL SETTINGS
# ============================================
# Get your PUID and PGID by running: id
# These MUST be the same across all services for symlinks to work
PUID=1000
PGID=1000
UMASK=002

# Timezone - Find yours at: https://en.wikipedia.org/wiki/List_of_tz_database_time_zones
TZ=America/New_York

# ============================================
# PLEX SETTINGS
# ============================================
# Optional: Plex claim token for initial setup
# Get from: https://www.plex.tv/claim/
# Leave empty if already set up
PLEX_CLAIM=

# ============================================
# REAL-DEBRID SETTINGS
# ============================================
# REQUIRED: Your Real-Debrid API token
# Get from: https://real-debrid.com/apitoken
REALDEBRID_API_KEY=your_realdebrid_api_key_here

# ============================================
# VPN SETTINGS (NordVPN via Gluetun)
# ============================================
# Service provider (nordvpn, etc.)
VPN_SERVICE_PROVIDER=nordvpn
# OpenVPN Credentials (if using OpenVPN)
VPN_USER=
VPN_PASSWORD=
# Wireguard Credentials (if using Wireguard/NordLynx - Recommended)
WIREGUARD_PRIVATE_KEY=
# Server regions (e.g. "Netherlands", "Switzerland")
SERVER_COUNTRIES=Switzerland

# ============================================
# ADVANCED SETTINGS
# ============================================
# Only change these if you know what you're doing

# Python settings
PYTHONUNBUFFERED=TRUE
